NAME	=	push_swap

CFLAGS	=	-Wall -Wextra -Werror -g -fsanitize=address 

CC		=	cc

SRCS	=	push_swap.c \
			init_push_swap.c \
			debug.c \
			operations/swap_push.c \
			operations/rotate.c \
			operations/r_rotate.c \
			parsing.c \
			utils.c \
			algorithm.c \
			sort_utils.c \

OBJS	=	$(SRCS:.c=.o)

all	: $(OBJS)
	make -C ./libft
	$(CC) $(CFLAGS) $(OBJS) ./libft/libft.a -o $(NAME)

$(NAME)	: all

